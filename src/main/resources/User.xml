<?xml version="1.0" encoding="UTF-8"?>
<config>

	<mode>debug</mode>

	<!-- 缓存 -->
	<cache>
		<!-- 是否开启本地持久化 -->
		<persist>true</persist>
		<!-- 持久化文件位置 -->
		<location>d:/cache</location>
	</cache>
	
	
	<select id="fetchAll" >
		select * from emp
	</select>

	<select id="selectByName" >
		select * from emp where ename =  #{ename} 
	</select>
	
</config>